{
	"name": "df_detalle_ventas_silver",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "detalles_ventas",
						"type": "DatasetReference"
					},
					"name": "ventasDetalles"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumnVentas"
				}
			],
			"scriptLines": [
				"source(output(",
				"          sls_ord_num as string,",
				"          sls_prd_key as string,",
				"          sls_cust_id as string,",
				"          sls_order_dt as string,",
				"          sls_ship_dt as string,",
				"          sls_sales as string,",
				"          sls_quantity as string,",
				"          sls_price as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> ventasDetalles",
				"ventasDetalles derive(ORD_NUM = upper(trim(sls_ord_num)),",
				"          PRD_KEY = upper(trim(sls_prd_key))) ~> derivedColumnVentas"
			]
		}
	}
}