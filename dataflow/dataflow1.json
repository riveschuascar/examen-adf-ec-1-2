{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "clientes",
						"type": "DatasetReference"
					},
					"name": "clientes"
				},
				{
					"dataset": {
						"referenceName": "clientes_bd_gen",
						"type": "DatasetReference"
					},
					"name": "clientesBdGen"
				},
				{
					"dataset": {
						"referenceName": "clientes_location",
						"type": "DatasetReference"
					},
					"name": "clientesLocation"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "derivedColumn3"
				},
				{
					"name": "joinClientesBdGen"
				},
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          cst_id as string,",
				"          cst_key as string,",
				"          cst_firstname as string,",
				"          cst_lastname as string,",
				"          cst_marital_status as string,",
				"          cst_gndr as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> clientes",
				"source(output(",
				"          CID as string,",
				"          BDATE as string,",
				"          GEN as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> clientesBdGen",
				"source(output(",
				"          CID as string,",
				"          CNTRY as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> clientesLocation",
				"clientes derive(CID_clean = replace(cst_key,' ',''),",
				"          FIRSTNAME = upper(trim(cst_firstname)),",
				"          LASTNAME = upper(trim(cst_lastname)),",
				"          MARITAL_STATUS = trim(cst_marital_status),",
				"          GENDER = trim(cst_gndr)) ~> derivedColumn1",
				"clientesBdGen derive(CID_clean = replace(CID,'NAS',''),",
				"          BDATE = toDate(BDATE,'yyyy-MM-dd'),",
				"          GENDER_BD = upper(trim(GEN))) ~> derivedColumn2",
				"clientesLocation derive(CID_clean = replace(CID,'-',''),",
				"          COUNTRY = upper(trim(CNTRY))) ~> derivedColumn3",
				"derivedColumn1, derivedColumn2 join(derivedColumn1@CID_clean == derivedColumn2@CID_clean,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinClientesBdGen",
				"joinClientesBdGen, derivedColumn3 join(clientesBdGen@CID == derivedColumn3@CID_clean,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1"
			]
		}
	}
}