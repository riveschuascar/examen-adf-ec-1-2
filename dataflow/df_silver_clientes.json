{
	"name": "df_silver_clientes",
	"properties": {
		"folder": {
			"name": "silver"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "table_customer",
						"type": "DatasetReference"
					},
					"name": "clientes"
				},
				{
					"dataset": {
						"referenceName": "table_customer_bdate_gen",
						"type": "DatasetReference"
					},
					"name": "clientesBdGen"
				},
				{
					"dataset": {
						"referenceName": "table_customer_location",
						"type": "DatasetReference"
					},
					"name": "clientesLocation"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "silver_clientes",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "derivedColumn3"
				},
				{
					"name": "joinClientesBdGen"
				},
				{
					"name": "joinFinal"
				},
				{
					"name": "select1"
				},
				{
					"name": "cast1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          cst_id as string,",
				"          cst_key as string,",
				"          cst_firstname as string,",
				"          cst_lastname as string,",
				"          cst_marital_status as string,",
				"          cst_gndr as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> clientes",
				"source(output(",
				"          cid as string,",
				"          bdate as string,",
				"          gen as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> clientesBdGen",
				"source(output(",
				"          cid as string,",
				"          cntry as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> clientesLocation",
				"clientes derive(cst_key = replace(cst_key,' ',''),",
				"          cst_firstname = trim(cst_firstname),",
				"          cst_lastname = trim(cst_lastname),",
				"          cst_marital_status = trim(cst_marital_status),",
				"          cst_gndr = trim(cst_gndr)) ~> derivedColumn1",
				"clientesBdGen derive(cid = replace(cid,'NAS',''),",
				"          bdate = toDate(bdate,'yyyy-MM-dd'),",
				"          gen = upper(trim(gen))) ~> derivedColumn2",
				"clientesLocation derive(cid = replace(cid,'-',''),",
				"          cntry = upper(trim(cntry))) ~> derivedColumn3",
				"derivedColumn1, derivedColumn2 join(cst_key == cid,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinClientesBdGen",
				"joinClientesBdGen, derivedColumn3 join(derivedColumn2@cid == derivedColumn3@cid,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinFinal",
				"joinFinal select(mapColumn(",
				"          cst_id,",
				"          cst_key,",
				"          cst_firstname,",
				"          cst_lastname,",
				"          cst_marital_status,",
				"          bdate,",
				"          cntry,",
				"          gen",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 cast(output(",
				"          cst_id as integer",
				"     ),",
				"     errors: true) ~> cast1",
				"cast1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          cst_id as integer,",
				"          cst_key as string,",
				"          cst_firstname as string,",
				"          cst_lastname as string,",
				"          cst_marital_status as string,",
				"          bdate as date,",
				"          gen as string,",
				"          cntry as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          cst_id,",
				"          cst_key,",
				"          cst_firstname,",
				"          cst_lastname,",
				"          cst_marital_status,",
				"          bdate,",
				"          gen,",
				"          cntry",
				"     )) ~> sink1"
			]
		}
	}
}